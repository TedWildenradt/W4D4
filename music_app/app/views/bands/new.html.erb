<h1>Add a Band</h1>

<form action="<%= bands_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label>
    Enter Band Name:
    <input type="text" name="band[name]" value=''>
  </label>

  <br>
  <input type="submit" value="Create Band">
</form>
